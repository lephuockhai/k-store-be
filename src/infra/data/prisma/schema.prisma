generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
enum USERSROLE {
  USER
  ADMIN
  SUPPERADMIN
  STORE
}

model User {
  id String @id @unique @default(uuid())
  first_name String
  last_name String
  email String @unique
  password String
  avatar String?
  role USERSROLE @default(USER)
  @@map("users")
}
